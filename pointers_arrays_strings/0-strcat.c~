#include "main.h"

/**
 * *_strcat - Entry point
 *
 * Description: copy un string par un pointeur
 * @dest: un string
 * @src: un string
 *
 * Return: Always 0 (Success)
 */


char *_strcat( char *dest, char *src)
{
char *c = dest;
while (*src != '\0')
{
*c = *src;
src++;
c++;
}
while (*dest != '\0')
{
*c = *dest;
dest++;
c++;
}
*c = '\0';
return (c);
}
