#include "main.h"

/**
 * _atoi - Entry point
 *
 * Description: 'the program's description'
 * @s: char
 *
 * Return: Always 0 (Success)
 */

int _atoi(char *s)
{
int i = 0;
 int j, k;
 char c;

// cherche la taille de *s stocke dans i
 
while (*s != '\0')
{
s++;
i++;
}

 for (j = 0; j <= i; j++)
   {
     if (*s == '-' || *s == '+')
       {
	 c = *s;
       }
     _putchar(c);
     if (*s >= '0' && *s <= '9')
       {
	 _putchar(s + '0');
       }
   }
 _putchar('\n');
 return (0);
}

